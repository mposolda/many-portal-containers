
<!-- Uncommented this when deploying sampleportal1 -->
<datasource jndi-name="java:/jdbcidm_sampleportal1" pool-name="IDMsampleportal1" enabled="true" use-java-context="true">
  <connection-url>jdbc:mysql://localhost/padsampleportal1</connection-url>
  <driver>mysql</driver>
  <security>
    <user-name>portal</user-name>
    <password>portal</password>
  </security>
  <transaction-isolation>TRANSACTION_READ_COMMITTED</transaction-isolation>
  <pool>
    <min-pool-size>2</min-pool-size>
    <max-pool-size>100</max-pool-size>
    <prefill>true</prefill>
  </pool>
  <statement>
    <prepared-statement-cache-size>100</prepared-statement-cache-size>
    <share-prepared-statements>true</share-prepared-statements>
  </statement>
</datasource>
<datasource jndi-name="java:/jdbcjcr_sampleportal1" pool-name="JCRsampleportal1" enabled="true" use-java-context="true">
  <connection-url>jdbc:mysql://localhost/padsampleportal1</connection-url>
  <driver>mysql</driver>
  <security>
    <user-name>portal</user-name>
    <password>portal</password>
  </security>
  <transaction-isolation>TRANSACTION_READ_COMMITTED</transaction-isolation>
  <pool>
    <min-pool-size>2</min-pool-size>
    <max-pool-size>100</max-pool-size>
    <prefill>true</prefill>
  </pool>
  <statement>
    <prepared-statement-cache-size>100</prepared-statement-cache-size>
    <share-prepared-statements>true</share-prepared-statements>
  </statement>
</datasource>


