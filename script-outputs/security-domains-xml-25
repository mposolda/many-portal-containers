
<!-- Uncommented this when deploying sample-portal1 -->
<security-domain name="gatein-domain-sample-portal1" cache-type="default">
  <authentication>
    <login-module code="org.gatein.security.oauth.jaas.OAuthLoginModule" flag="required">
      <module-option name="portalContainerName" value="sample-portal1"/>
      <module-option name="realmName" value="gatein-domain-sample-portal1"/>
    </login-module>
    <login-module code="org.gatein.sso.integration.SSODelegateLoginModule" flag="required">
      <module-option name="enabled" value="#{gatein.sso.login.module.enabled}" />
      <module-option name="delegateClassName" value="#{gatein.sso.login.module.class}" />
      <module-option name="portalContainerName" value="sample-portal1" />
      <module-option name="realmName" value="gatein-domain-sample-portal1" />
      <module-option name="password-stacking" value="useFirstPass" />
    </login-module>
    <login-module code="org.exoplatform.services.security.j2ee.JBossAS7LoginModule" flag="required">
      <module-option name="portalContainerName" value="sample-portal1"/>
      <module-option name="realmName" value="gatein-domain-sample-portal1"/>
    </login-module>
  </authentication>
</security-domain>

